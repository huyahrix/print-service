{
  "name": "electron-with-express",
  "description": "Demonstrates spawning Express from Electron and using it to serve content",
  "version": "0.15.0",
  "main": "main.js",
  "authors": [
    "Frank Hale <frankhale@gmail.com>"
  ],
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "start": ".\\node_modules\\node\\bin\\node.exe start-electron.js",
    "package-win-portable": "npx electron-builder build --win=portable",
    "package-win": "npx electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/print.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE",
    "package": "npx electron-packager . --overwrite --platform=win32 --arch=x64 --icon=assets/icons/win/print.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron-With-Express\""
  },
  "dependencies": {
    "electron-builder": "^22.10.5",
    "electron-pos-printer": "^1.2.4",
    "lodash": "^4.17.20",
    "node": "^10.18.0",
    "request": "^2.88.2",
    "strip": "^3.0.0"
  },
  "devDependencies": {
    "electron": "11.2.0"
  }
}
